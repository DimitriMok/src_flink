<?xml version="1.0" encoding="UTF-8" ?>
<configuration status="INFO" MonitorInterval="30">
	<Appenders>
		<Console name="console">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss}-%-5p %l: %m %n"/>
		</Console>
	</Appenders>

	<Loggers>
		<Root level="INFO">
			<AppenderRef ref="console"/>
		</Root>
	</Loggers>



</configuration>
